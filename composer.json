{
  "name": "reem/plagiarism-detection-system",
  "description": "Plagiarism Detection System",
  "type": "project",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Models\\": "Models/",
      "Controllers\\": "Controllers/",
      "Helpers\\": "Helpers/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}